{
  "parser": "babel-eslint",
  "extends": [
    "eslint-config-airbnb"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/wrap-multilines": 0,
    "func-names": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-unused-expressions": 0,
    "consistent-return": 0,
    "no-empty": 0,
    "no-use-before-define": 0,
    "camelcase": 0,
    "default-case": 0,
    "no-console": 0,
    "id-length": 0
  },
  "globals": {
    "filepicker": true,
    "dispatcher": true,
    "snowplow": true,
    "optimizely": true,
    "analytics": true,
    "FB": true,
    "CT": true,
    "globals": true
  }
}
